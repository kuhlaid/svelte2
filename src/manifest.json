{
  "manifest_version": "0.1",
  "name": " kuhlaid / svelte2",
  "version" : "0.1",
  "default_locale": "en",
  "description": "Svelte example (not Svelte version 2) with basic Bootstrap styling and navigation to give a 'multi-page' feel. Repository found at https://github.com/kuhlaid/svelte2.",
  "short_name": "svelte2",
  "lang": "en-US",
  "start_url": "index.html",
  "display": "standalone",
  "theme_color": "#90a8b2",
  "background-color": "white",
  "icons": [
    {
      "src": "images/touch/icon-128x128.png",
      "sizes": "128x128"
    },
    {
      "src": "images/touch/icon-192x192.png",
      "sizes": "192x192"
    },
    {
      "src": "images/touch/icon-256x256.png",
      "sizes": "256x256"
    },
    {
      "src": "images/touch/icon-384x384.png",
      "sizes": "384x384"
    },
    {
      "src": "images/touch/icon-512x512.png",
      "sizes": "512x512"
    }
  ]
}
